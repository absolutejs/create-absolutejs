{
	"author": "Alex Kahn",
	"bin": {
		"create-absolutejs": "dist/index.js"
	},
	"dependencies": {
		"@clack/prompts": "0.11.0",
		"picocolors": "1.1.1"
	},
	"description": "A CLI tool to create a new AbsoluteJS project",
	"devDependencies": {
		"@stylistic/eslint-plugin-ts": "4.2.0",
		"@types/bun": "latest",
		"@types/react": "19.1.4",
		"autoprefixer": "10.4.21",
		"drizzle-kit": "0.31.1",
		"drizzle-orm": "0.41.0",
		"eslint": "9.27.0",
		"eslint-plugin-absolute": "0.1.4",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-promise": "7.2.1",
		"eslint-plugin-security": "3.0.1",
		"prettier": "3.5.3",
		"prettier-plugin-svelte": "3.4.0",
		"svelte": "5.35.2",
		"tailwindcss": "4.1.7",
		"typescript-eslint": "8.32.0",
		"vue": "3.5.17"
	},
	"files": [
		"dist",
		"dist/templates"
	],
	"license": "CC BY-NC 4.0",
	"main": "dist/index.js",
	"name": "create-absolutejs",
	"peerDependencies": {
		"typescript": "5.8.3"
	},
	"scripts": {
		"build": "rm -rf dist && tsc --project tsconfig.build.json && cp -R src/templates dist/templates",
		"dev": "rm -rf absolutejs-project && bun run src/index.ts",
		"format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,json,mjs,md,svelte,html,vue}\"",
		"lint": "eslint ./",
		"release": "bun run format && bun run build && bun publish",
		"test": "bash -c 'trap \"exit 0\" INT; cd absolutejs-project && bun dev'",
		"typecheck": "bun run tsc --noEmit"
	},
	"type": "module",
	"version": "0.3.9"
}
